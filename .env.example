# Supabase Configuration
# Get these from Supabase Project Settings -> API
VITE_SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL_HERE
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_PUBLIC_KEY_HERE
SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL_HERE
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_SECRET_HERE

# Note: Use SERVICE_ROLE_KEY because the server needs admin privileges (manage users, bypass RLS).
# DO NOT EXPOSE SERVICE_ROLE_KEY TO THE FRONTEND (CLIENT).
# Only VITE_SUPABASE_URL and VITE_SUPABASE_ANON_KEY should be used in frontend code.

# Server Port
PORT=3000

# Other settings
WITHDRAWAL_AUTO_FAIL_MS=300000
